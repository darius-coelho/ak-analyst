{
  "name": "ak-analyst",
  "version": "1.2.0",
  "description": "Akai Kaeru's ai pattern miner and explorer",
  "main": "electron/public/electron.js",
  "author": "darius coelho",
  "license": "ISC",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^27.2.0",
    "@ag-grid-community/core": "^27.2.0",
    "@ag-grid-community/react": "^27.2.0",
    "@babel/runtime": "^7.18.9",
    "@electron/remote": "^2.0.1",
    "@material-ui/core": "^4.12.1",
    "@testing-library/react": "^12.0.0",
    "array-move": "^4.0.0",
    "axios": "^0.22.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bootstrap": "^5.1.0",
    "canvg": "^4.0.0",
    "crossfilter": "^1.3.12",
    "css-loader": "^5.2.7",
    "d3": "^7.6.1",
    "d3-sankey": "^0.7.1",
    "d3-scale-chromatic": "^1.1.1",
    "electron-load-balancer": "^3.0.0",
    "electron-log": "^3.0.5",
    "es6-promise": "^4.0.5",
    "jeezy": "^1.13.1",
    "jest": "^27.0.6",
    "jquery": "^3.6.0",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.4",
    "mixpanel-browser": "^2.45.0",
    "node-machine-id": "^1.1.12",
    "portastic": "^1.0.1",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-bootstrap": "^2.0.0-beta.5",
    "react-bootstrap-validation": "^0.1.11",
    "react-colorful": "^5.5.1",
    "react-confirm-alert": "^2.8.0",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^16.14.0",
    "react-drag-drop-container": "^6.1.1",
    "react-draggable": "^4.4.3",
    "react-dropdown-select": "^4.7.4",
    "react-dropzone": "^11.4.2",
    "react-error-boundary": "^3.1.4",
    "react-file-input": "^0.2.5",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "react-sortable-hoc": "^2.0.0",
    "react-split-pane": "^0.1.92",
    "react-svg-brush": "^1.0.0",
    "react-svg-to-image": "^3.0.0",
    "react-transition-group": "^2.2.1",
    "save": "^2.4.0",
    "socket.io-client": "^4.2.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "ws": "^8.2.3"
  },
  "scripts": {
    "test": "jest --env=jsdom",
    "react-electron-start": "cross-env NODE_ENV=development webpack serve --content-base electron/public --inline --hot --port 3000 --config ./electron/webpack.config.js",
    "electron-start": "cross-env DEV=1 electron .",
    "electron-start-exe": "cross-env DEV=1 exe=1 electron .",
    "start": "concurrently \"npm run react-electron-start\" \"wait-on http://localhost:3000/ && npm run electron-start\"",
    "start-exe": "concurrently \"npm run react-electron-start\" \"wait-on http://localhost:3000/ && npm run electron-start-exe\"",
    "react-electron-build": "webpack --config electron/webpack.config.js --mode production",
    "pack": "electron-builder --dir -c.extraMetadata.main=electron/build/electron.js",
    "electron-build-linux": "electron-builder --linux -c.extraMetadata.main=electron/public/electron.js",
    "electron-build-windows": "electron-builder --win -c.extraMetadata.main=electron/public/electron.js",
    "electron-build-mac": "electron-builder --mac -c.extraMetadata.main=electron/public/electron.js",
    "build-linux": "npm run react-electron-build && npm run electron-build-linux",
    "build-windows": "npm run react-electron-build && npm run electron-build-windows",
    "build-mac": "npm run react-electron-build && npm run electron-build-mac",
    "react-web-start": "cross-env NODE_ENV=development webpack serve --content-base web/public --inline --hot --port 3000 --config ./web/webpack.config.js",
    "react-web-build": "webpack --config web/webpack.config.js --mode production",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.17.12",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/user-event": "^13.2.1",
    "babel-eslint": "9.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "devtron": "^1.4.0",
    "electron": "^16.0.4",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "5.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "husky": "^1.3.1",
    "prettier": "^1.17.0",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "AK Analyst",
    "productName": "AK Analyst",
    "copyright": "Akai Kaeru",
    "asar": true,
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility",
      "maintainer": "epapenha@akaikaeru.com"
    },
    "appImage": {
      "license": "./assets/license_en.txt"
    },
    "snap": {
      "confinement": "classic"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icons/icon.ico",
      "legalTrademarks": "AK",
      "signingHashAlgorithms": [
        "sha1",
        "sha256"
      ]
    },
    "mac": {
      "icon": "assets/icons/icon.png",
      "category": "public.app-category.productivity",
      "target": [
        "dmg"
      ]
    },
    "nsis": {
      "installerIcon": "assets/icons/icon.ico",
      "installerHeaderIcon": "assets/icons/icon.ico",
      "license": "assets/license_en.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "dmg": {
      "icon": "assets/icons/icon.png",
      "iconSize": 80,
      "iconTextSize": 12,
      "title": "AK Analyst"
    },
    "files": [
      "electron/public/**/*",
      "node_modules/**/*",
      "assets/**/*"
    ],
    "directories": {
      "output": "electron/dist",
      "buildResources": "assets"
    },
    "extraResources": [
      {
        "from": "background_tasks/app",
        "to": "app",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extends": null
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.5",
    "utf-8-validate": "^5.0.7"
  }
}
